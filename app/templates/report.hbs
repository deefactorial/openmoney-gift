<div id="{{currency}}" class="statcard statcard-{{#currencyObject}}{{currency_color}}{{/currencyObject}} p-a-md m-b">
  <h1> {{#currencyObject}}{{currency_name}}{{/currencyObject}} </h1>
  <h3 class="statcard-number text-right">
    BALANCE {{decimal_places balance}}

  </h3>
  <h3 class="statcard-number text-right">
    VOLUME {{decimal_places volume}}
  </h3>
</div>
<div class="col-sm-6" style="padding-bottom: 20px;">
  <div class="form-group text-left">
    <label for="startDate">Start Date:</label>
    <div class="input-with-icon">
      <input type="text" name="startDate" value="{{startDate}}" placeholder="Start Date" class="form-control">
      <span class="icon icon-calendar"></span>
    </div>
  </div>
</div>
<div class="col-sm-6" style="padding-bottom: 20px;">
  <div class="form-group text-left">
    <label for="endDate">End Date:</label>
    <div class="input-with-icon">
      <input type="text" name="endDate" value="{{endDate}}" placeholder="End Date" class="form-control">
      <span class="icon icon-calendar"></span>
    </div>
  </div>
</div>
<div class="table-full">
  <table class="table display" data-sort="table">
    <thead>
      <tr>
        <th class="">Date</th>
        <th class="">Patron</th>
        <th class="">Employee</th>
        <th class="">Card Number</th>
        <th class="">Load or Redeem</th>
        <th class="">Amount</th>
        <!-- <th class="">By Employee</th> -->
      </tr>
    </thead>
    <tfoot>
      <tr>
        <th>&nbsp;</th>
        <th>&nbsp;</th>
        <th>&nbsp;</th>
        <th>&nbsp;</th>
        <th style="text-align:right"><div>Balance:</div><div>Volume:</div></th>
        <th><div>{{decimal_places balance}}</div><div>{{decimal_places volume}}</div></th>
      </tr>
    </tfoot>
    <tbody class="table-rows">
      {{#each journals}}
        {{#this}}
        <tr id="{{_id}}">
          <td>{{prettify_date_short timestamp}}</td>
          <td><a href="#merchants/{{#merchant}}{{merchantname}}{{/merchant}}/patrons/{{cardholderID}}">{{patron}}</a></td>
          <td>{{employee}}</td>
          <td><a href="#merchants/{{#merchant}}{{merchantname}}{{/merchant}}/patrons/{{cardholderID}}/cards/{{key}}">{{key}}</a></td>
          <td>{{#load}}Load{{/load}}{{#redeem}}Redeem{{/redeem}}</td>
          <td>{{decimal_places amount}}</td>
          <!-- <td>{{employeeID}}</td> -->
        </tr>
        {{/this}}
      {{/each}}
    </tbody>
  </table>
</div>
